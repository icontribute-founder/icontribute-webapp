    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "clean": "rimraf dist",
        "build": "npm run clean && tsc",
        "test:base": "mocha -r ts-node/register ./tests/**/*.test.ts --exit",
        "test:prep": "npm run build",
        "test": "npm run test:prep && firebase emulators:exec --import=tests/data 'npm run test:base'",
        "start:emulator": "firebase emulators:start",
        "install-firebase-cli": "npm install -g firebase-tools",
        "docs": "typedoc --options typedoc.json"
    },
    "keywords": [
        "iContribute"
    ],
