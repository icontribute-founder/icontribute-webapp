        "@types/mocha": "^9.0.0",
        "@types/node": "^16.10.2",
        "chai": "^4.3.4",
        "mocha": "^9.1.2",
        "rimraf": "^3.0.2",
        "ts-node": "^10.2.1",
        "typedoc": "^0.20.0",
        "typedoc-neo-theme": "^1.1.0",
        "typescript": "~4.2.0"
    }
